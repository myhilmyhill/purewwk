{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WorkingDirectory": null,
  "MusicDirectory": "/app/Music",
  "FFmpeg": {
    "Path": "ffmpeg"
  },
  "LosslessEncoding": {
    "Enabled": true,
    "TargetFormat": "m4a",
    "AudioCodec": "aac",
    "AudioBitrate": "320k",
    "AudioSampleRate": null,
    "LosslessFormats": ["flac", "wav", "ape", "wv", "alac", "aiff", "au"]
  },
  "HlsCache": {
    "MaxSize": 100,
    "MaxAgeMinutes": 60,
    "Enabled": true
  },
  "FileWatcher": {
    "Enabled": true
  }
}
